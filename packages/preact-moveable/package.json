{
    "name": "preact-moveable",
    "version": "0.1.6",
    "description": "A Preact Component that create Moveable, Draggable, Resizable, Scalable, Rotatable.",
    "main": "./dist/moveable.cjs.js",
    "module": "./dist/moveable.esm.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "start": "open ./demo/index.html && rollup -c rollup.config.demo.js -w",
        "build": "rollup -c && npm run declaration && print-sizes ./dist ",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
    },
    "keywords": [
        "moveable",
        "resizable",
        "scalable",
        "draggable",
        "rotatable",
        "movable",
        "dom",
        "resize",
        "scale",
        "drag",
        "move",
        "rotate",
        "react"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/daybrush/moveable/blob/master/packages/preact-moveable"
    },
    "author": "Daybrush",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/daybrush/moveable/issues"
    },
    "homepage": "https://github.com/daybrush/moveable#readme",
    "devDependencies": {
        "@daybrush/builder": "^0.1.0",
        "@scenejs/effects": "^1.0.1",
        "keycon": "^0.3.0",
        "preact": "^8.4.2",
        "print-sizes": "^0.1.0",
        "rollup-plugin-css-bundle": "^1.0.4",
        "rollup-plugin-preact": "^0.3.0",
        "scenejs": "^1.1.4",
        "shape-svg": "^0.3.3",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5"
    },
    "dependencies": {
        "@daybrush/drag": "^0.5.0",
        "@daybrush/utils": "^0.10.0",
        "framework-utils": "^0.1.0",
        "preact-css-styler": "^0.3.0",
        "react-moveable": "^0.2.2"
    }
}
